<p id="notice"><%= notice %></p>

<h1>Listing Tipo Culinaria</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_culinaria.each do |tipo_culinarium| %>
      <tr>
        <td><%= tipo_culinarium.nome %></td>
        <td><%= link_to 'Show', tipo_culinarium %></td>
        <td><%= link_to 'Edit', edit_tipo_culinarium_path(tipo_culinarium) %></td>
        <td><%= link_to 'Destroy', tipo_culinarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo culinarium', new_tipo_culinarium_path %>
