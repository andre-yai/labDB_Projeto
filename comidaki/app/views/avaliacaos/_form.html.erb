<%= simple_form_for(@avaliacao) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :replica %>
    <%= f.input :data %>
    <%= f.input :comentario %>
    <%= f.input :nota %>
    <%= f.input :cpf %>
    <%= f.input :cnpf %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
