<p id="notice"><%= notice %></p>

<h1>Listing Avaliacaos</h1>

<table>
  <thead>
    <tr>
      <th>Replica</th>
      <th>Data</th>
      <th>Comentario</th>
      <th>Nota</th>
      <th>Cpf</th>
      <th>Cnpf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @avaliacaos.each do |avaliacao| %>
      <tr>
        <td><%= avaliacao.replica %></td>
        <td><%= avaliacao.data %></td>
        <td><%= avaliacao.comentario %></td>
        <td><%= avaliacao.nota %></td>
        <td><%= avaliacao.cpf %></td>
        <td><%= avaliacao.cnpf %></td>
        <td><%= link_to 'Show', avaliacao %></td>
        <td><%= link_to 'Edit', edit_avaliacao_path(avaliacao) %></td>
        <td><%= link_to 'Destroy', avaliacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

