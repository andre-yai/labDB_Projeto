<%= simple_form_for(@endereco) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :logradouro %>
    <%= f.input :cep %>
    <%= f.input :numero %>
    <%= f.input :complemento %>
    <%= f.input :cidade %>
    <%= f.input :estado %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
