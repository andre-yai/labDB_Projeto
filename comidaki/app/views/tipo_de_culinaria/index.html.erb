<p id="notice"><%= notice %></p>

<h1>Listing Tipo De Culinaria</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_de_culinaria.each do |tipo_de_culinaria| %>
      <tr>
        <td><%= tipo_de_culinaria.nome %></td>
        <td><%= link_to 'Show', tipo_de_culinaria %></td>
        <td><%= link_to 'Edit', edit_tipo_de_culinaria_path(tipo_de_culinaria) %></td>
        <td><%= link_to 'Destroy', tipo_de_culinaria, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo de culinaria', new_tipo_de_culinaria_path %>
